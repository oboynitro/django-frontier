{
	"name": "frontier",
	"version": "0.1.0",
	"description": "Front-end scaffolding for django",
	"license": "MIT",
	"keywords": [
		"django",
		"scaffold",
		"frontend",
		"react",
		"vue",
		"tailwindcss",
		"bootstrap"
	],
	"repository": "https://github.com/oboynitro/django-frontier.git",
	"homepage": "https://django-frontier.readthedocs.io/en/latest/",
	"author": "Frank Owusu Boakye <oboy.nitro1@gmail.com>",
	"scripts": {
		"dev": "rm -rf .cache static/dist && parcel watch resources/js/index.js -d static/dist",
		"build": "rm -rf .cache static/build && parcel build resources/js/index.js -d static/build"
	},
	"dependencies": {
		"tailwindcss": "^1.9.6",
		"react": "^17.0.1",
		"react-dom": "^17.0.1"
	},
	"devDependencies": {
		"@babel/core": "^7.12.3",
		"@babel/preset-env": "^7.12.1",
		"@babel/preset-react": "^7.12.5",
		"babel-loader": "^8.2.1",
		"parcel-bundler": "^1.12.4"
	}
}
